#include <stdio.h>
#include <string.h>

void	*ft_memcpy(void *dst, const void *src, size_t n)
{
	unsigned char *tmp_str;
	unsigned char *tmp_str2;

	tmp_str = (unsigned char *)src;
	tmp_str2 = (unsigned char *)dst;
	while(n--)
		*tmp_str2++ = *tmp_str++;
	return(dst);
}

int main()
{
	char *dst = strdup("2222");
	unsigned char src[10]= "1234567890";
	printf("%s\n",ft_memcpy(&src[1], &src[3], 12));
	printf("%s\n", memcpy(&src[1], &src[3], 12));
}